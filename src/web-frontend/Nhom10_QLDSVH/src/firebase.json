{
    "functions": {
      "source": "."
    },
    "hosting": {
      "public": "public",
      "rewrites": [
        {
          "source": "/models/**",
          "function": "corsProxy"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    "storage": {
        "rules": "storage.rules" 
      }
  }
  